<addon id="repository.silhouette.matrix" name="Silhouette Kodi Matrix Add-ons" version="1.1.1" provider-name="Silhouette">
  <requires>
    <import addon="xbmc.addon" version="12.0.0"/>
  </requires>
	<extension point="xbmc.addon.repository" name="Silhouette Kodi Matrix Add-ons">
		<dir minversion="19.0.0">
            <info compressed="false">https://github.com/Silhouette2022/kodi.matrix/raw/main/addons.xml</info>
            <checksum>https://github.com/Silhouette2022/kodi.matrix/raw/main/addons.xml.md5</checksum>
            <datadir zip="true">https://github.com/Silhouette2022/kodi.matrix/raw/main/repo/</datadir>
		</dir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Add-ons for Kodi starting from Matrix rev</summary>
		<description>Дополнения для Коди начиная с версии Матрица</description>
		<disclaimer>For home usage only</disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
